		"bits-ui": "^1.3.17",
		"clsx": "^2.1.1",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.1.0",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.0",
